generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Wilaya {
  id         Int         @id @default(autoincrement())
  nom_fr     String      @db.VarChar(100)
  nom_ar     String      @db.VarChar(100)
  code       String?     @unique(map: "code") @db.VarChar(10)
  createdAt  DateTime    @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt  DateTime    @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  moughataas Moughataa[]
  users      User[]

  @@map("wilayas")
}

model Moughataa {
  id        Int       @id @default(autoincrement())
  nom_fr    String    @db.VarChar(100)
  nom_ar    String    @db.VarChar(100)
  code      String?   @unique(map: "code") @db.VarChar(10)
  wilayaId  Int       @map("wilaya_id")
  createdAt DateTime  @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt DateTime  @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  communes  Commune[]
  wilaya    Wilaya    @relation(fields: [wilayaId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "moughataas_ibfk_1")

  @@index([wilayaId], map: "idx_moughataas_wilaya")
  @@map("moughataas")
}

model Commune {
  id          Int       @id @default(autoincrement())
  nom_fr      String    @db.VarChar(100)
  nom_ar      String    @db.VarChar(100)
  code        String?   @unique(map: "code") @db.VarChar(10)
  moughataaId Int       @map("moughataa_id")
  createdAt   DateTime  @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt   DateTime  @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  centres     Centre[]
  moughataa   Moughataa @relation(fields: [moughataaId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "communes_ibfk_1")

  @@index([moughataaId], map: "idx_communes_moughataa")
  @@map("communes")
}

model Centre {
  id        Int      @id @default(autoincrement())
  nom_fr    String   @db.VarChar(100)
  nom_ar    String   @db.VarChar(100)
  code      String?  @unique(map: "code") @db.VarChar(10)
  communeId Int      @map("commune_id")
  createdAt DateTime @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt DateTime @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  bureaux   Bureau[]
  commune   Commune  @relation(fields: [communeId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "centres_ibfk_1")

  @@index([communeId], map: "idx_centres_commune")
  @@map("centres")
}

model Bureau {
  id             Int        @id @default(autoincrement())
  nom_fr         String     @db.VarChar(100)
  nom_ar         String     @db.VarChar(100)
  nombreInscrits Int?       @default(0) @map("nombre_inscrits")
  nombreVotants  Int?       @default(0) @map("nombre_votants")
  centreId       Int        @map("centre_id")
  createdAt      DateTime   @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt      DateTime   @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  centre         Centre     @relation(fields: [centreId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "bureaux_ibfk_1")
  resultats      Resultat[]

  @@index([centreId], map: "idx_bureaux_centre")
  @@map("bureaux")
}

model Candidat {
  id        Int        @id @default(autoincrement())
  nom_fr    String     @unique @db.VarChar(100)
  nom_ar    String     @db.VarChar(100)
  photo     String?    @db.VarChar(255)
  logo      String?    @db.VarChar(255)
  couleur   String     @db.VarChar(7)
  actif     Boolean?   @default(true)
  createdAt DateTime   @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt DateTime   @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  resultats Resultat[]

  @@map("candidats")
}

model ElectionType {
  id           Int        @id @default(autoincrement())
  nom_fr       String     @db.VarChar(100)
  nom_ar       String     @db.VarChar(100)
  dateElection DateTime?  @map("date_election") @db.Date
  actif        Boolean?   @default(true)
  createdAt    DateTime   @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt    DateTime   @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  resultats    Resultat[]

  @@map("election_types")
}

model User {
  id                 Int      @id @default(autoincrement())
  name               String   @db.VarChar(100)
  email              String   @unique(map: "email") @db.VarChar(100)
  password           String   @db.VarChar(255)
  role               UserRole @default(GUEST)
  wilayaId           Int?     @map("wilaya_id")
  avatar             String?  @db.VarChar(255)
  createdAt          DateTime @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt          DateTime @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  mustChangePassword Boolean  @default(true)
  wilaya             Wilaya?  @relation(fields: [wilayaId], references: [id], onUpdate: Restrict, map: "users_ibfk_1")

  @@index([wilayaId], map: "idx_users_wilaya")
  @@map("users")
}

model Resultat {
  id             Int          @id @default(autoincrement())
  electionTypeId Int          @map("election_type_id")
  bureauId       Int          @map("bureau_id")
  candidatId     Int          @map("candidat_id")
  nombreVoix     Int?         @default(0) @map("nombre_voix")
  voixNuls       Int?         @default(0) @map("voix_nuls")
  voixRejetes    Int?         @default(0) @map("voix_rejetes")
  createdAt      DateTime     @default(now()) @map("created_at") @db.Timestamp(0)
  updatedAt      DateTime     @default(now()) @updatedAt @map("updated_at") @db.Timestamp(0)
  electionType   ElectionType @relation(fields: [electionTypeId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "resultats_ibfk_1")
  bureau         Bureau       @relation(fields: [bureauId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "resultats_ibfk_2")
  candidat       Candidat     @relation(fields: [candidatId], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "resultats_ibfk_3")

  @@unique([electionTypeId, bureauId, candidatId], name: "unique_result", map: "unique_result")
  @@index([bureauId], map: "idx_resultats_bureau")
  @@index([candidatId], map: "idx_resultats_candidat")
  @@index([electionTypeId], map: "idx_resultats_election")
  @@map("resultats")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model geo_temp {
  Wilaya       String? @db.VarChar(18)
  MoughataaFr  String? @db.VarChar(16)
  Commune      String? @db.VarChar(24)
  Center       String? @db.VarChar(45)
  bureau       String? @db.VarChar(48)
  Wilaya_ar    String? @db.VarChar(17)
  MoughataaAr  String? @db.VarChar(12)
  Commune_ar   String? @db.VarChar(20)
  Center_ar    String? @db.VarChar(48)
  bureau_ar    String? @db.VarChar(51)
  nbr_Inscrits Int?

  @@ignore
}

enum UserRole {
  ADMIN    @map("admin")
  OPERATOR @map("operator")
  GUEST    @map("guest")

  @@map("user_role")
}
